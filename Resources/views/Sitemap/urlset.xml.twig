<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for link in links %}
        <url>
            <loc>{{ link.loc }}</loc>
            <lastmod>{{ link.lastmod }}</lastmod>
            <changefreq>{{ link.changefreq }}</changefreq>
            <priority>{{ link.priority }}</priority>
        </url>
    {% endfor %}
</urlset>